{
  "format_version": "1.8.0",
  "render_controllers": {
    "controller.render.hive.party.screen": {
      "arrays": {
        "textures": {
          "array.textures": [
            "Texture.background_lobby",
            "Texture.background_warmup",
            "Texture.background_waiting",
            "Texture.background_reveal",
            "Texture.background_floor_removed",
            "Texture.background_visualizer",
            "Texture.background_game_over"
          ]
        }
      },
      "geometry": "Geometry.screen",
      "ignore_lighting": true,
      "light_color_multiplier": 0.92,
      "materials": [
        {
          "*": "Material.screen"
        }
      ],
      "textures": [
        "array.textures[query.mark_variant]"
      ]
    },
    "controller.render.hive.party.screen.reveal": {
      "arrays": {
        "textures": {
          "array.textures": [
            "Texture.reveal_white",
            "Texture.reveal_white_cb",
            "Texture.reveal_orange",
            "Texture.reveal_orange_cb",
            "Texture.reveal_magenta",
            "Texture.reveal_magenta_cb",
            "Texture.reveal_light_blue",
            "Texture.reveal_light_blue_cb",
            "Texture.reveal_yellow",
            "Texture.reveal_yellow_cb",
            "Texture.reveal_lime",
            "Texture.reveal_lime_cb",
            "Texture.reveal_pink",
            "Texture.reveal_pink_cb",
            "Texture.reveal_gray",
            "Texture.reveal_gray_cb",
            "Texture.reveal_light_gray",
            "Texture.reveal_light_gray_cb",
            "Texture.reveal_cyan",
            "Texture.reveal_cyan_cb",
            "Texture.reveal_purple",
            "Texture.reveal_purple_cb",
            "Texture.reveal_blue",
            "Texture.reveal_blue_cb",
            "Texture.reveal_brown",
            "Texture.reveal_brown_cb",
            "Texture.reveal_green",
            "Texture.reveal_green_cb",
            "Texture.reveal_red",
            "Texture.reveal_red_cb",
            "Texture.reveal_black",
            "Texture.reveal_black_cb"
          ]
        }
      },
      "geometry": "Geometry.reveal",
      "materials": [
        {
          "*": "Material.screen"
        }
      ],
      "textures": [
        "array.textures[query.variant]"
      ]
    },
    "controller.render.hive.party.screen.visualizer": {
      "geometry": "Geometry.visualizer",
      "ignore_lighting": true,
      "light_color_multiplier": 0.92,
      "materials": [
        {
          "*": "Material.screen"
        }
      ],
      "part_visibility": [
        {
          "*": "query.mark_variant == 5"
        }
      ],
      "textures": [
        "Texture.visualizer"
      ]
    }
  }
}
