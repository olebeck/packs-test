{
  "animation_controllers": {
    "controller.animation.hive.sky.spawn.vehicle": {
      "states": {
        "default": {
          "transitions": [
            {
              "sorcery": "query.variant == 20"
            },
            {
              "ghastly": "query.variant == 21"
            },
            {
              "solar_orbit": "query.variant == 23"
            },
            {
              "hoverbike": "query.variant == 24"
            },
            {
              "pegasus": "query.variant == 25"
            }
          ]
        },
        "ghastly": {
          "animations": [
            "ghastly_idle",
            "ghastly_legs"
          ],
          "particle_effects": [
            {
              "effect": "ghastly_fire",
              "locator": "righteye"
            },
            {
              "effect": "ghastly_fire",
              "locator": "lefteye"
            }
          ]
        },
        "hoverbike": {
          "animations": [
            "hoverbike_idle"
          ]
        },
        "pegasus": {
          "animations": [
            "pegasus_idle"
          ],
          "particle_effects": [
            {
              "effect": "pegasus",
              "locator": "leftwing"
            },
            {
              "effect": "pegasus",
              "locator": "rightwing"
            }
          ]
        },
        "solar_orbit": {
          "animations": [
            "solar_orbit_idle",
            "solar_orbit_rocket"
          ],
          "particle_effects": [
            {
              "effect": "solar_orbit",
              "locator": "sun"
            }
          ]
        },
        "sorcery": {
          "animations": [
            "sorcery_idle",
            "sorcery_book"
          ],
          "particle_effects": [
            {
              "effect": "sorcery_magic",
              "locator": "root"
            },
            {
              "effect": "sorcery_enchantment",
              "locator": "book"
            }
          ]
        }
      }
    }
  },
  "format_version": "1.10.0"
}
